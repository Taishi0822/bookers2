<h2>User info</h2>
    <a><%= image_tag user.get_profile_image(100,100) %></a>
    <table>
        
        <tr>
            <th>name</th>
            <th><%= user.name %></th>
        </tr>
        <tr>
            <th>introduction</th>
            <th><%= user.introduction %></th>
        </tr>
    </table>
    <a><%= link_to "edit", edit_user_path(user) %>
     <i class="fas fa-user-cog"></i>
    </a>
    